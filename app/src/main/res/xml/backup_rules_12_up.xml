<?xml version="1.0" encoding="utf-8"?>

<!-- Back up user data with Auto Backup -->
<!-- https://developer.android.com/guide/topics/data/autobackup -->
<data-extraction-rules>
  <cloud-backup>
    <exclude
      domain="sharedpref"
      path="."
      />
  </cloud-backup>
  <device-transfer>
    <exclude
      domain="sharedpref"
      path="."
      />
  </device-transfer>
</data-extraction-rules>
